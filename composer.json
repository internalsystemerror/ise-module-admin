{
    "name": "ise/ise-module-admin",
    "description": "Administration module for Zend Framework 2 used by Internalsystemerror Limited",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/internalsystemerror/ise-module-admin",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "classmap-authoritative": true,
        "discard-changes": true,
        "optimize-autoloader": true,
        "prepend-autoloader": true,
        "process-timeout": 5000,
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:internalsystemerror/zf-doctrine-data-fixture.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "ise/ise-module-bootstrap": "dev-develop",
        "ise/ise-module-bread": "dev-develop",
        "zendframework/zend-navigation": "^2.9.0",
        "zf-commons/zfc-rbac": "^2.6.3",
        "zf-commons/zfc-user-doctrine-orm": "^2.0.0"
    },
    "require-dev": {
        "api-skeletons/zf-doctrine-data-fixture": "dev-feature/symfony-cli",
        "ise/php-metapackage-development": "^2.0.2"
    },
    "suggest": {
        "api-skeletons/zf-doctrine-data-fixture": "To use doctrine fixtures"
    },
    "autoload": {
        "psr-4": {
            "Ise\\Admin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IseTest\\Admin\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.1.x-dev"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse -l 7 -c phpstan.neon config src",
        "cs-check": [
            "phpcs",
            "phpmd ./config,./src,./test,./view html --reportfile ./build/logs/phpmd.html --suffixes php,phtml ./phpmd.xml.dist",
            "phpcpd . --exclude=build --exclude=vendor"
        ],
        "cs-fix": "phpcbf",
        "test": "phpunit -c ./phpunit.xml.dist"
    }
}
